{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:all": "concurrently -k -p \"[{name}]\" -n \"API,QLOAPPS-INBOUND,QLOAPPS-OUTBOUND,QLOAPPS-SYNC\" -c \"cyan,green,yellow,magenta\" \"node dist/src/server.js\" \"node dist/src/workers/qloapps_inbound_worker.js\" \"node dist/src/workers/qloapps_outbound_worker.js\" \"node dist/src/workers/qloapps_sync_scheduler.js\"",
    "dev": "nodemon --exec tsx src/server.ts",
    "dev:all": "concurrently -k -p \"[{name}]\" -n \"API,QLOAPPS-INBOUND,QLOAPPS-OUTBOUND,QLOAPPS-SYNC\" -c \"cyan,green,yellow,magenta\" \"npm run dev\" \"npm run worker:qloapps-inbound\" \"npm run worker:qloapps-outbound\" \"npm run worker:qloapps-scheduler\"",
    "worker:qloapps-inbound": "tsx src/workers/qloapps_inbound_worker.ts",
    "worker:qloapps-outbound": "tsx src/workers/qloapps_outbound_worker.ts",
    "worker:qloapps-scheduler": "tsx src/workers/qloapps_sync_scheduler.ts",
    "worker:inbound": "tsx src/workers/inbound_worker.ts",
    "worker:outbound": "tsx src/workers/outbound_worker.ts",
    "worker:sync": "tsx src/workers/sync_scheduler.ts",
    "script:fix-orphaned-rooms": "tsx scripts/fix_orphaned_rooms_hotel_id.ts",
    "db:migrate": "knex migrate:latest --knexfile knexfile.ts",
    "db:migrate:make": "knex migrate:make --knexfile knexfile.ts -x ts",
    "db:migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "db:migrate:rollback:all": "knex migrate:rollback --knexfile knexfile.ts --all",
    "db:migrate:status": "knex migrate:status --knexfile knexfile.ts",
    "db:seed": "knex seed:run --knexfile knexfile.ts",
    "db:seed:make": "knex seed:make --knexfile knexfile.ts -x ts",
    "migrate": "npm run db:migrate",
    "seed": "npm run db:seed",
    "test": "vitest",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "bcrypt": "^6.0.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "knex": "^3.1.0",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "@types/node": "^25.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/amqplib": "^0.10.8",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/knex": "^0.15.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  }
}
